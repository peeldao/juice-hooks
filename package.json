{
  "name": "juice-hooks",
  "version": "5.0.17-beta",
  "description": "Wagmi hooks for Juicebox",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "generate-addresses": "ts-node --project tsconfig.cjs.json ./scripts/generateAddresses.ts",
    "pregenerate": "npm run generate-addresses",
    "generate": "wagmi generate",
    "build": "tsup && tsc --emitDeclarationOnly"
  },
  "author": "tomquirk",
  "license": "ISC",
  "devDependencies": {
    "@jbx-protocol/juice-contracts-v3": "^5.0.0",
    "@types/react": "^18.2.21",
    "@wagmi/cli": "^1.5.1",
    "dotenv": "^16.3.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "peerDependencies": {
    "wagmi": "^1.3.10"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "fpnum": "^0.0.13",
    "react": "^18.2.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  }
}
